{
  "kind": "collectionType",
  "collectionName": "extras",
  "info": {
    "name": "Extra"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "ingredient": {
      "via": "extra",
      "model": "ingredient"
    }
  }
}
